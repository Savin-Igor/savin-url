{
    "name": "savin/url",
    "description": "Thin wrapper over the library sabre/uri",
    "license": "MIT",
    "authors": [
        {
            "name": "Savin Igor",
            "email": "igors.savins@inbox.lv",
            "homepage": "https://github.com/Savin-Igor"
        }
    ],
    "homepage": "https://github.com/Savin-Igor/savin-url",
    "keywords": ["Laravel", "url", "tools", "helpers", "util"],
